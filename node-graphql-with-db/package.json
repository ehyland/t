{
  "type": "module",
  "scripts": {
    "dev": "ENV=dev tsx watch src/index.ts",
    "start:dev": "ENV=dev node dist/index.js",
    "studio": "ENV=dev drizzle-kit studio",
    "build": "tsup-node",
    "gen": "pnpm run '/^(gen:graphql|gen:migration)$/'",
    "gen:graphql": "pnpm run '/^gen:graphql:.*/'",
    "gen:graphql:api": "graphql-codegen --config graphql.config.ts --project api",
    "gen:graphql:test-client": "graphql-codegen --config graphql.config.ts --project test-client",
    "gen:migration": "ENV=dev drizzle-kit generate",
    "typecheck": "tsc",
    "lint": "biome check",
    "format": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "test": "vitest",
    "test:ci": "CI=true vitest",
    "check": "pnpm run '/^(typecheck|lint|format|test:ci)$/'"
  },
  "dependencies": {
    "@graphql-yoga/plugin-disable-introspection": "^2.6.3",
    "better-sqlite3": "^11.1.2",
    "close-with-grace": "^1.3.0",
    "cookie": "^0.6.0",
    "cookie-signature": "^1.2.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "emittery": "^1.0.3",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.6.3",
    "ix": "^7.0.0",
    "ky": "^1.5.0",
    "morgan": "^1.10.0",
    "remeda": "^2.10.1",
    "tiny-invariant": "^1.3.3",
    "ulid": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-document-nodes": "4.0.9",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-resolvers": "4.2.1",
    "@parcel/watcher": "^2.4.1",
    "@types/better-sqlite3": "^7.6.11",
    "@types/cookie": "^0.6.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.1.0",
    "@types/ws": "^8.5.12",
    "drizzle-kit": "^0.24.0",
    "env-cmd": "^10.1.0",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "graphql-config": "^5.1.0",
    "graphql-sse": "^2.5.3",
    "msw": "^2.3.5",
    "outdent": "^0.8.0",
    "prettier": "^3.3.3",
    "supertest-fetch": "^2.0.0",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "undici": "^6.19.6",
    "utility-types": "^3.11.0",
