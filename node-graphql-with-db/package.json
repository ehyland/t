{
  "type": "module",
  "scripts": {
    "dev": "ENV=dev tsx watch src/index.ts",
    "start:dev": "ENV=dev node dist/index.js",
    "studio": "ENV=dev drizzle-kit studio",
    "build": "tsup-node",
    "gen": "pnpm run '/^(graphql:gen:.*)$/'",
    "gen:watch": "pnpm run '/^(graphql:watch:.*)$/'",
    "graphql:gen:api": "graphql-codegen --config graphql.config.ts --project api",
    "graphql:gen:test-client": "graphql-codegen --config graphql.config.ts --project test-client",
    "graphql:watch:api": "graphql-codegen --config graphql.config.ts --project api --watch",
    "graphql:watch:test-client": "graphql-codegen --config graphql.config.ts --project test-client --watch",
    "db:generate-migration": "ENV=dev drizzle-kit generate",
    "db:run-migrations": "ENV=dev drizzle-kit migrate",
    "typecheck": "tsc",
    "lint": "biome check",
    "format": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "test": "vitest",
    "test:ci": "CI=true vitest",
    "check": "pnpm run '/^(typecheck|lint|format|test:ci)$/'"
  },
  "dependencies": {
    "@graphql-yoga/plugin-disable-introspection": "^2.10.8",
    "@libsql/client": "^0.14.0",
    "close-with-grace": "^2.1.0",
    "cookie": "^1.0.2",
    "cookie-signature": "^1.2.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "emittery": "^1.0.3",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.10.8",
    "ix": "^7.0.0",
    "ky": "^1.7.4",
    "morgan": "^1.10.0",
    "remeda": "^2.19.0",
    "tiny-invariant": "^1.3.3",
    "ulid": "^2.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.2",
    "@graphql-codegen/typescript-document-nodes": "4.0.12",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@graphql-codegen/typescript-resolvers": "4.4.1",
    "@parcel/watcher": "^2.5.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "drizzle-kit": "^0.30.1",
    "env-cmd": "^10.1.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "graphql-config": "^5.1.3",
    "graphql-sse": "^2.5.3",
    "msw": "^2.7.0",
    "outdent": "^0.8.0",
    "prettier": "^3.4.2",
    "supertest-fetch": "^2.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "undici": "^7.2.0",
    "utility-types": "^3.11.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.2"
}
